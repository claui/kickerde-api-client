    <objects>
        <document id="1062330" sportId="1" ressortId="4000" date="2024-10-25T15:25:55" title="Faktor 100 f&#xFC;r Haas: Crash-Kurs im Keller f&#xFC;r den Talente-Schmied" teaser="Einstand an diesem Samstag in Gelsenkirchen: Leonhard Haas, der neue Trainer von Greuther F&#xFC;rth, kann sich bis zur Winterpause empfehlen. Als St&#xFC;rmer begann er einst beim FC Bayern, als Coach erhielt er den Feinschliff in Salzburg." liveblog="Off" adKeywords="sportart=1" imageModul="https://derivates.kicker.de/image/upload/c_crop,x_259,y_43,w_2731,h_1536/q_auto,w_800/v1/2024/10/25/85272eb6-63f2-496d-b98b-4389ebbc1dd4.jpeg" imageModulWidth="800" imageModulHeight="450" typId="13" typName="ARTIKEL" header="Der Werdegang des neuen F&#xFC;rther Trainers" imageText="Leonhard Haas bei der SpVgg Greuther F&#xFC;rth." sportsId="1" isPlusContent="1"/>
        <document id="1062328" sportId="1" ressortId="4000" date="2024-10-25T14:42:19" title="&quot;Wir haben in jedem Spiel Druck&quot;: SCP-Coach Hildmann will Kellerduell nicht &#xFC;berh&#xF6;hen" teaser="Vor dem wichtigen Spiel gegen Eintracht Braunschweig versucht Sascha Hildmann, den Spielern von Preu&#xDF;en M&#xFC;nster ein wenig den Druck zu nehmen. Au&#xDF;erdem lobt er den Gegner und spricht dar&#xFC;ber, wie ein Erfolg gegen die L&#xF6;wen gelingen kann." liveblog="Off" adKeywords="sportart=1" imageModul="https://derivates.kicker.de/image/upload/w_3944,h_2218,c_fill,g_auto:subject/q_auto,w_800/v1/2024/10/25/dc2a8c0f-d23e-4246-afed-d6d3b4292e60.jpeg" imageModulWidth="800" imageModulHeight="450" typId="13" typName="ARTIKEL" header="Der 17. spielt gegen den 16." imageText="Sascha Hildmann steht seit Januar 2020 bei M&#xFC;nster an der Seitenlinie." sportsId="1" isPlusContent="0"/>
        <document id="1062325" sportId="1" ressortId="4000" date="2024-10-25T13:37:17" title="Premiere f&#xFC;r van Wonderen &quot;mit einem L&#xE4;cheln im Gesicht&quot;" teaser="Im ersten Spiel erlebte Schalkes neuer Trainer Kees van Wonderen einen ziemlich blutleeren Auftritt seiner Mannschaft. Zur Heim-Premiere gegen F&#xFC;rth am Samstag werden alle Kr&#xE4;fte geb&#xFC;ndelt." liveblog="Off" adKeywords="sportart=1" imageModul="https://derivates.kicker.de/image/upload/w_3000,h_1687,c_fill,g_auto:subject/q_auto,w_800/v1/2024/10/25/6c38e994-c158-4c03-9fca-19fe50f6d6a6.jpeg" imageModulWidth="800" imageModulHeight="450" typId="13" typName="ARTIKEL" header="Schalke-Coach sucht vor dem F&#xFC;rth-Spiel die Balance" imageText="Schalkes neuer Trainer Kees van Wonderen freut sich auf sein Heim-Deb&#xFC;t." sportsId="1" isPlusContent="0"/>
        <document id="1062323" sportId="1" ressortId="4000" date="2024-10-25T13:33:42" title="Kohfeldt: Keine Garantien und keine Denkverbote" teaser="Innerhalb einer guten Woche bestreitet der SV Darmstadt 98 sein zweites Heimspiel hintereinander. Gegen Aufsteiger Ulm erwartet Trainer Florian Kohfeldt jedoch ein komplett anderes Spiel als gegen Absteiger K&#xF6;ln - und warnt sein Team." liveblog="Off" adKeywords="sportart=1" imageModul="https://derivates.kicker.de/image/upload/c_crop,x_381,y_363,w_3552,h_1998/q_auto,w_800/v1/2024/10/25/31bdee19-852a-4e42-9e96-590ff6606011.jpeg" imageModulWidth="800" imageModulHeight="450" typId="13" typName="ARTIKEL" header="Darmstadt will gegen Ulm den zweiten Heimsieg hintereinander" imageText="Darmstadts Coach Florian Kohfeldt warnt vor Aufsteiger Ulm." sportsId="1" isPlusContent="0"/>
        <document id="1062300" sportId="1" ressortId="4000" date="2024-10-25T12:47:10" title="Enochs mit deutlichen Worten und Zweckoptimismus" teaser="Joe Enochs ist der Meinung, dass Jahn Regensburg trotz der 0:3-Niederlage gegen D&#xFC;sseldorf kleine Schritte nach vorne gemacht hat. Vor der Partie gegen N&#xFC;rnberg gibt er Einblicke in taktische &#xDC;berlegungen und lobt die ungebrochene Unterst&#xFC;tzung der Fans." liveblog="Off" adKeywords="sportart=1" imageModul="https://derivates.kicker.de/image/upload/c_crop,x_526,y_456,w_2857,h_1607/q_auto,w_800/v1/2024/10/25/83edb1b6-52e2-4e71-901c-f6108092af8b.jpeg" imageModulWidth="800" imageModulHeight="450" typId="13" typName="ARTIKEL" header="&quot;Wir sind verdammt, Ergebnisse zu holen&quot;" imageText="Joe Enochs bei der Ansprache im Mannschaftskreis des SSV Jahn Regensburg nach der Niederlage gegen D&#xFC;sseldorf." sportsId="1" isPlusContent="0"/>
        <document id="1062024" sportId="1" ressortId="4000" date="2024-10-25T05:45:00" title="Tsch&#xFC;ss, Vollgas-Fu&#xDF;ball: Baumgarts Flexibilit&#xE4;t ist Hamburgs neue Erfolgsformel" teaser="Gegen Elversberg gab Steffen Baumgart sein Deb&#xFC;t als HSV-Trainer, drang aber trotz eines Sieges noch nicht zu den Spielern durch. Vor dem Wiedersehen mit den Saarl&#xE4;ndern hat sich dies ver&#xE4;ndert, der Trainer wird nun wegen seiner neuen Flexibilit&#xE4;t allseits gesch&#xE4;tzt." liveblog="Off" adKeywords="sportart=1" imageModul="https://derivates.kicker.de/image/upload/c_crop,x_0,y_142,w_4000,h_2250/q_auto,w_800/v1/2024/10/24/5b6f9b80-4a52-4fbd-9bcd-330416e1dab2.jpeg" imageModulWidth="800" imageModulHeight="450" typId="13" typName="ARTIKEL" header="Spieler und Trainer haben zueinander gefunden" imageText="Inniger Jubel: Steffen Baumgart inmitten seiner Spieler." sportsId="1" isPlusContent="1"/>
